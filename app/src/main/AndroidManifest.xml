<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.triviaapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/appscrip_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/appscrip_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Controller.History"></activity>
        <activity android:name=".Controller.Summary" />
        <activity android:name=".Controller.Question3" />
        <activity android:name=".Controller.Question2" />
        <activity android:name=".Controller.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Controller.MainActivity" />
    </application>

</manifest>